class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        
        int cnt=0;
        int size=nums.size();
        for(int i=0;i<nums.size();i++)
        {
            if(nums[i]==0)
            {
                cnt++;
            }
        }
        for(int j=0;j<cnt;j++)
        {
            for(int i=0;i<size-j-1;i++)
            {
                if(nums[i]==0)
                {
                    swap(nums[i],nums[i+1]);
                }
            }
        }
        
        return;
    }
};
